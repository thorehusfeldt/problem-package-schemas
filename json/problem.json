{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$ref": "#/$defs/Problem",
    "openapi": "3.0.0",
    "info": {
        "title": "Generated by cue, with some handmade changes by Thore",
        "version": "no version"
    },
    "paths": {},
    "$defs": {
            "LanguageCode": {
                "type": "string",
                "pattern": "^[a-z]{2,3}(-[A-Z]{2})?$"
            },
            "Limits": {
                "type": "object",
                "properties": {
                    "time_multipliers": {
                        "type": "object",
                        "properties": {
                            "ac_to_time_limit": {
                                "type": "number",
                                "default": 2.0
                            },
                            "time_limit_to_tle": {
                                "type": "number",
                                "default": 1.5
                            }
                        }
                    },
                    "time_resolution": {
                        "type": "number",
                        "minimum": 0,
                        "exclusiveMinimum": true,
                        "default": 1.0
                    },
                    "validation_passes": {
                        "type": "integer",
                        "minimum": 0,
                        "exclusiveMinimum": true,
                        "default": 1
                    }
                }
            },
            "Person": {
                "type": "string"
            },
            "Persons": {
                "oneOf": [
                    {
                        "type": "string"
                    },
                    {
                        "type": "array",
                        "minItems": 1,
                        "items": [
                            {
                                "$ref": "#/$defs/Person"
                            }
                        ],
                        "additionalItems": {
                            "$ref": "#/$defs/Person"
                        }
                    }
                ]
            },
            "Problem": {
                "type": "object",
                "required": [
                    "problem_format_version",
                    "name",
                    "uuid"
                ],
                "properties": {
                    "problem_format_version": {
                        "type": "string",
                        "oneOf": [
                            {
                                "enum": [
                                    "draft",
                                    "legacy",
                                    "legacy-icpc"
                                ]
                            },
                            {
                                "pattern": "^[0-9]{4}-[0-9]{2}(-draft)?$"
                            }
                        ]
                    },
                    "type": {
                        "default": "pass-fail",
                        "oneOf": [
                            {
                                "enum": [
                                    "pass-fail",
                                    "scoring",
                                    "multi-pass",
                                    "interactive",
                                    "submit-answer"
                                ]
                            },
                            {
                                "type": "array",
                                "minItems": 1,
                                "items": [
                                    {
                                        "$ref": "#/$defs/Type"
                                    }
                                ],
                                "additionalItems": {
                                    "$ref": "#/$defs/Type"
                                }
                            }
                        ]
                    },
                    "name": {
                        "oneOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "object"
                            }
                        ]
                    },
                    "uuid": {
                        "type": "string"
                    },
                    "version": {
                        "type": "string"
                    },
                    "credits": {
                        "oneOf": [
                            {
                                "$ref": "#/$defs/Person"
                            },
                            {
                                "type": "object",
                                "required": [
                                    "authors"
                                ],
                                "properties": {
                                    "authors": {
                                        "$ref": "#/$defs/Persons"
                                    },
                                    "translators": {
                                        "type": "object"
                                    }
                                }
                            }
                        ]
                    },
                    "source": {
                        "oneOf": [
                            {
                                "$ref": "#/$defs/Source"
                            },
                            {
                                "type": "array",
                                "minItems": 1,
                                "items": [
                                    {
                                        "$ref": "#/$defs/Source"
                                    }
                                ],
                                "additionalItems": {
                                    "$ref": "#/$defs/Source"
                                }
                            }
                        ]
                    },
                    "license": {
                        "type": "string",
                        "enum": [
                            "unknown",
                            "public domain",
                            "cc0",
                            "cc by",
                            "cc by-sa",
                            "educational",
                            "permission"
                        ],
                        "default": "unknown"
                    },
                    "rights_owner": {
                        "type": "string"
                    },
                    "embargo_until": {
                        "type": "string",
                        "format": "date"
                    },
                    "limits": {
                        "$ref": "#/$defs/Limits"
                    },
                    "keywords": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "languages": {
                        "default": "all",
                        "oneOf": [
                            {
                                "enum": [
                                    "all"
                                ]
                            },
                            {
                                "type": "array",
                                "minItems": 1,
                                "items": [
                                    {
                                        "$ref": "#/$defs/ProgrammingLanguage"
                                    }
                                ],
                                "additionalItems": {
                                    "$ref": "#/$defs/ProgrammingLanguage"
                                }
                            }
                        ]
                    },
                    "allow_file_writing": {
                        "type": "boolean",
                        "enum": [
                            false,
                            true
                        ],
                        "default": false
                    },
                    "constants": {
                        "type": "object"
                    }
                }
            },
            "ProgrammingLanguage": {
                "type": "string",
                "enum": [
                    "ada",
                    "algol68",
                    "apl",
                    "bash",
                    "c",
                    "cgmp",
                    "cobol",
                    "cpp",
                    "cppgmp",
                    "crystal",
                    "csharp",
                    "d",
                    "dart",
                    "elixir",
                    "erlang",
                    "forth",
                    "fortran",
                    "fsharp",
                    "gerbil",
                    "go",
                    "haskell",
                    "java",
                    "javaalgs4",
                    "javascript",
                    "julia",
                    "kotlin",
                    "lisp",
                    "lua",
                    "modula2",
                    "nim",
                    "objectivec",
                    "ocaml",
                    "octave",
                    "odin",
                    "pascal",
                    "perl",
                    "php",
                    "prolog",
                    "python2",
                    "python3",
                    "python3numpy",
                    "racket",
                    "ruby",
                    "rust",
                    "scala",
                    "simula",
                    "smalltalk",
                    "snobol",
                    "swift",
                    "typescript",
                    "visualbasic",
                    "zig"
                ]
            },
            "SizeLimit": {
                "type": "string",
                "enum": [
                    "memory",
                    "output",
                    "code",
                    "compilation_memory",
                    "validation_memory",
                    "validation_output"
                ]
            },
            "Source": {
                "oneOf": [
                    {
                        "type": "string"
                    },
                    {
                        "type": "object",
                        "required": [
                            "name"
                        ],
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "url": {
                                "type": "string"
                            }
                        }
                    }
                ]
            },
            "TimeLimit": {
                "type": "string",
                "enum": [
                    "time_limit",
                    "compilation_time",
                    "validation_time"
                ]
            },
            "Type": {
                "type": "string",
                "enum": [
                    "pass-fail",
                    "scoring",
                    "multi-pass",
                    "interactive",
                    "submit-answer"
                ]
            }
    }
}
